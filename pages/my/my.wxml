<view class="container">
  <view class="myHeader">
    <view class="myLogo"><image src="{{userInfo.avatarUrl}}"></image></view>
    <view class="myName">
      <view class="myNick">{{userInfo.nickName}}</view>
      <view class="myBind">绑定小试牛刀账号</view>
    </view>
    <view class="myEwm"><image src="../../images/funbtn05.png"></image></view>
  </view>
  <view class='myOrder'>
    <view class='myOrder-block' bindtap='goMyorder' data-current="0">
      <image class='myOrder-img' src='../../images/myorder3.png'></image>
      <text class='myOrder-text'>待付款</text>
    </view>
    <view class='myOrder-block' bindtap='goMyorder' data-current="1">
      <image class='myOrder-img' src='../../images/myorder.png'></image>
      <text class='myOrder-text'>待收货</text>
    </view>
    <view class='myOrder-block' bindtap='goMyorder' data-current="2">
      <image class='myOrder-img' src='../../images/myorder6.png'></image>
      <text class='myOrder-text'>已完成</text>
    </view>
    <view class='myOrder-block' bindtap='goMyorder' data-current="3">
      <image class='myOrder-img' src='../../images/myorder32.png'></image>
      <text class='myOrder-text'>全部订单</text>
    </view>
  </view>
  <view class="myList">
    <view><image src="../../images/myorder.png"></image><text>我的订单</text></view>
    <view><image src="../../images/myorder6.png"></image><text>我的优惠券</text></view>
    <view bindtap='goAddress'><image src="../../images/myorder3.png"></image><text>收货地址管理</text></view>
  </view>
  <view class="mySetting"><image src="../../images/setting.png"></image><text>设置</text></view>
</view>